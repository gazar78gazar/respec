{
  "name": "requirements-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"src/refactored/**/*.ts*\" --ignore-pattern \"**/__tests__/**\" --fix",
    "ts:check": "npx tsc --noEmit",
    "format": "prettier --write \"src/refactored/**/*.ts*\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:quick": "node -e \"console.log('Open browser console and paste test-runner.js content')\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^12.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "husky": "^8.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^7.3.0",
    "vitest": "^0.34.0"
  }
}
